{
    "name": "silverback/fattureincloud",
    "type": "wordpress-plugin",
    "description": "FattureInCloud API PHP SDK",
    "keywords": ["sdk", "api", "rest", "web service", "client", "fattureincloud"],
    "homepage": "https://github.com/silverbackstudio/fattureincloud/",
    "license": "MIT",
    "authors": [
        {
            "name": "Brando Meniconi    ",
            "email": "b.meniconi@silverbackstudio.it",
            "homepage": "https://github.com/silverbackstudio"
        }
    ],
    "require": {
        "php": ">=5.6",
        "psr/log": "^1.0",
        "guzzlehttp/guzzle": "~6.0",
        "monolog/monolog": "^1.23"        
    },
    "require-dev": {
        "ext-curl": "*",
        "phpunit/phpunit": "^4.0 || ^5.0",
        "psr/log": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Svbk\\FattureInCloud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Svbk\\FattureInCloud\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    }
}
